{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:app": "next dev --turbopack",
    "dev:sb": "storybook dev -p 6006",
    "dev:sb:static": "serve storybook-static/index.html",
    "debug": "next dev --turbopack & set NODE_OPTIONS='--inspect'",
    "build:app": "next build",
    "build:sb": "storybook build",
    "start": "next start",
    "lint:ts": "next lint",
    "lint:scss": "stylelint \"**/*.{css,scss}\" --fix --ignore-path .gitignore",
    "test:unit": "jest",
    "test:unit:clear": "rm -rf \"src/config/tests/unit/results\"",
    "test:endToEnd": "playwright test",
    "test:endToEnd:clear": "rm -rf \"src/config/tests/endToEnd/results\"",
    "test:snapshot": "test-storybook",
    "test:snapshot:clear": "rm -rf \"src/config/tests/snapshot/results\"",
    "test:show": "playwright show-report",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@mui/material-nextjs": "^7.1.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@svgr/webpack": "^8.1.0",
    "next": "15.4.0",
    "next-redux-wrapper": "^8.1.0",
    "next-router-mock": "^1.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "react-redux": "^9.2.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.53.1",
    "@storybook/addon-a11y": "^9.0.14",
    "@storybook/nextjs": "9.0.12",
    "@stylistic/stylelint-plugin": "^3.1.3",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.34",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.30.0",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-fsd-import": "^0.0.13",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "9.0.8",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "30.0.0-beta.8",
    "sass": "^1.89.2",
    "scss-reset": "^1.4.6",
    "storybook": "9.0.8",
    "stylelint": "^16.21.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "stylelint-order-config-standard": "^0.1.3",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
